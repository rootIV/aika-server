unit Objects;

interface

uses
  Windows, SysUtils, MiscData;

{$REGION 'OBJECTS Threads'}

{$ENDREGION}

{$REGION 'Objects Data'}
type
  POBJ = ^TOBJ;

  TOBJ = record
    Index: WORD;
    Position: TPosition;
    ContentType: Byte;
    ContentAmount: WORD;
    ContentCollectTime: WORD;
    ReSpawn: Boolean;
  end;
{$ENDREGION}


implementation

end.
